{
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:powershell"
  ],
  "categories": [
    "Programming Languages",
    "AI"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aidocswriter.generateDocstring",
        "title": "Gemini Docstrings: Generate Docstring"
      },
      {
        "command": "aidocswriter.changeModel",
        "title": "Gemini Docstrings: Change Model"
      }
    ],
    "configuration": {
      "properties": {
        "aidocswriter.apiKey": {
          "description": "Your Google Gemini API key.",
          "type": "string"
        },
        "aidocswriter.model": {
          "default": "gemini-2.5-flash-lite",
          "description": "The API endpoint for the Gemini model.",
          "enum": [
            "gemini-2.5-pro",
            "gemini-2.5-flash",
            "gemini-2.5-flash-lite",
            "gemini-2.0-flash",
            "gemini-2.0-flash-lite"
          ],
          "type": "string"
        }
      },
      "title": "Gemini Docstring Generator Configuration"
    }
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "description": "Generate docstrings using AI for multiple languages.",
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/node-fetch": "^2.6.9",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.9.2"
  },
  "displayName": "Gemini Docstrings Writer",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "assets/icon.png",
  "main": "./out/extension.js",
  "name": "aidocswriter",
  "publisher": "amburgao",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amburgao/aidocswriter"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "1.0.1"
}
